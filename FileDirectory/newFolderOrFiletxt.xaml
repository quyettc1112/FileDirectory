<Window x:Class="FileDirectory.newFolderOrFiletxt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileDirectory"
        mc:Ignorable="d"
        Title="newFolderOrFiletxt" Height="300" Width="400">
    <Grid>
        <TextBox x:Name="tb_fof" HorizontalAlignment="Center" Margin="0,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="314"/>
        <Button x:Name="btn_savefof" Content="Save" HorizontalAlignment="Center" Margin="0,167,0,0" VerticalAlignment="Top" Click="btn_savefof_Click"/>
    </Grid>
</Window>
